<%if(hexo.theme.config.google_analytics){%>
    <!-- Global site tag (gtag.js) - Google Analytics -->
    <script async src="https://www.googletagmanager.com/gtag/js?id=<%= hexo.theme.config.google_analytics%>"></script>
    <script>
        window.dataLayer = window.dataLayer || [];
        function gtag() { dataLayer.push(arguments); }
        gtag('js', new Date());

        gtag('config', '<%= hexo.theme.config.google_analytics%>');
    </script>
<%}%>

